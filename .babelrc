{
  "presets": [
    "react",
    "env",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-object-assign",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "public": "public",
        "components": "components",
        "componentsShare": "components/share",
        "utils": "utils",
        "constants": "constants"
      }
    }]
  ]
}
